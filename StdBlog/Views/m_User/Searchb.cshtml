@model IEnumerable<StdBlog.Models.m_Blog>

<div class="panel panel-info">
    <div class="panel-heading">Blog</div>
    <div class="list-group">
        @if (Model.Count() == 0)
        {
            @(new HtmlString("<p class=\"list - group - item\">No result!</p>"))
        }
        @foreach (var t in Model)
        {
            @Html.ActionLink(t.title, "ShowBlog", "m_Blog", new {id=t.ID }, new {@class= "list-group-item" });
        }
    </div>
</div>
