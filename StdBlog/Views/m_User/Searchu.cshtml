@model IEnumerable<StdBlog.Models.m_User>

<div class="panel panel-info">
    <div class="panel-heading">User</div>
    <div class="list-group">
        @if (Model.Count() == 0)
        {
            @(new HtmlString("<p class=\"list - group - item\">No result!</p>"))
        }
        @foreach (var t in Model)
        {
            @Html.ActionLink(t.name, "UserBinfo", "m_User", new { id = t.ID }, new { @class = "list-group-item" });
        }
    </div>
</div>
